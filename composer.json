{
  "name": "eufony/orm",
  "description": "Model-based zero-configuration PHP object relational mapping library.",
  "keywords": [ "eufony", "orm", "sql" ],
  "type": "library",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Alpin Gencer",
      "email": "alpingencer@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^8.0"
  },
  "require-dev": {
    "fakerphp/faker": "^1.16",
    "mockery/mockery": "^1.4",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Eufony\\ORM\\": [ "src/Exception/", "src/" ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\": [ "tests/App/" ],
      "Eufony\\ORM\\Tests\\": [ "tests/" ]
    }
  },
  "scripts": {
    "test": [
      "phpunit -c tests/Unit/phpunit.xml.dist",
      "phpunit -c tests/Integration/phpunit.xml.dist"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
